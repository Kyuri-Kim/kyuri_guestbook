    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kyuri.kyuri_guestbook.mapper">

	<select id="selectAllList" resultType="com.kyuri.kyuri_guestbook.domain.Guestbook">
